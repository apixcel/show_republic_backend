{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/new/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/new/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/new",
  "projects": {
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication/tsconfig.app.json"
      }
    },
    "gamification": {
      "type": "application",
      "root": "apps/gamification",
      "entryFile": "main",
      "sourceRoot": "apps/gamification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gamification/tsconfig.app.json"
      }
    },
    "http-gateway": {
      "type": "application",
      "root": "apps/http-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/http-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/http-gateway/tsconfig.app.json"
      }
    },
    "category": {
      "type": "application",
      "root": "apps/category",
      "entryFile": "main",
      "sourceRoot": "apps/category/src",
      "compilerOptions": {
        "tsConfigPath": "apps/category/tsconfig.app.json"
      }
    }
  }
}
