  "scripts": {
    "start:dev": "concurrently -n ADMIN,AUTHENTICATION,BRAND,CATEGORY,GAMEFICATION,HTTP-GATEWAY,LIVE,NOTIFICATION,PLAYLIST, POST-COMMENT,POST-LIKE-TOGGLE,POSTS,PRODUCT,PROFILE,PROMOTION,ROLE-PERMISSION,SETTINGS,STATISTICS,SUBSCRIPTION,UPLOADFILE, WALLET,YOUR-CHANNEL \"npm run watch:admin\" \"npm run watch:authentication\" \"npm run watch:brand\" \"npm run watch:category\" \"npm run watch:gamefication\" \"npm run watch:httpGateway\" \"npm run watch:live\" \"npm run watch:notification\" \"npm run watch:playlist\" \"npm run watch:post-comment\" \"npm run watch:post-like-toggle\" \"npm run watch:posts\" \"npm run watch:product\" \"npm run watch:profile\" \"npm run watch:promotion\" \"npm run watch:role-permission\" \"npm run watch:settings\" \"npm run watch:statistics\" \"npm run watch:subscription\" \"npm run watch:uploadfile\" \"npm run watch:wallet\" \"npm run watch:your-channel\"",
    "watch:admin": "nodemon --config apps/admin/nodemon.json",
    "watch:authentication": "nodemon --config apps/authentication/nodemon.json",
    "watch:brand": "nodemon --config apps/brand/nodemon.json",
    "watch:category": "nodemon --config apps/category/nodemon.json",
    "watch:gamefication": "nodemon --config apps/gamefication/nodemon.json",
    "watch:httpGateway": "nodemon --config apps/http-gateway/nodemon.json",
    "watch:live": "nodemon --config apps/live/nodemon.json",
    "watch:notification": "nodemon --config apps/notification/nodemon.json",
    "watch:playlist": "nodemon --config apps/playlist/nodemon.json",
    "watch:post-comment": "nodemon --config apps/post-comment/nodemon.json",
    "watch:post-like-toggle": "nodemon --config apps/post-like-toggle/nodemon.json",
    "watch:posts": "nodemon --config apps/posts/nodemon.json",
    "watch:product": "nodemon --config apps/product/nodemon.json",
    "watch:profile": "nodemon --config apps/profile/nodemon.json",
    "watch:promotion": "nodemon --config apps/promotion/nodemon.json",
    "watch:role-permission": "nodemon --config apps/role-permission/nodemon.json",
    "watch:settings": "nodemon --config apps/settings/nodemon.json",
    "watch:statistics": "nodemon --config apps/statistics/nodemon.json",
    "watch:subscription": "nodemon --config apps/subscription/nodemon.json",
    "watch:uploadfile": "nodemon --config apps/uploadfile/nodemon.json",
    "watch:wallet": "nodemon --config apps/wallet/nodemon.json",
    "watch:your-channel": "nodemon --config apps/your-channel/nodemon.json",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "create-service": "node create_service.js",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/new/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/new/test/jest-e2e.json",
    "migration:create": "mikro-orm migration:create --config=libs/config/src/lib/postgresdb.config.ts",
    "migration:up": "mikro-orm migration:up --config=libs/config/src/lib/postgresdb.config.ts"
  },
